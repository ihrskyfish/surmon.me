{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "LICENSE": true,
    "public": true,
    "scripts": true,
    "patches": true,
    ".github": true,
    "dist": true,
    ".editorconfig": true,
    "relatedProject": true,
    "sbundler": true,
    "screenshots": true,
    "src/assets": true,
    "src/styles/base": true,
    "src/styles": true,
    "src/language/lang-zh.ts": true,
    "src/language/lang-en.ts": true,
    "src/language/key.ts": true,
    "src/language": true
  },
  "hide-files.files": [
    "LICENSE",
    "public",
    "scripts",
    "patches",
    ".github",
    "dist",
    ".editorconfig",
    "relatedProject",
    "sbundler",
    "screenshots",
    "src/assets",
    "src/styles/base",
    "src/styles",
    "src/language/lang-zh.ts",
    "src/language/lang-en.ts",
    "src/language/key.ts",
    "src/language",
    "src/constants"
  ]
}
